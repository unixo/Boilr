<fieldset>
    <legend>Dati anagrafici</legend>

    {{ form_row(form.type) }}
    {{ form_row(form.title) }}
    {{ form_row(form.name) }}
    {{ form_row(form.surname) }}
    {{ form_row(form.fiscalCode) }}
    {{ form_row(form.vatCode) }}
</fieldset>

<fieldset>
    <legend>Recapiti</legend>

    {{ form_row(form.homePhone) }}
    {{ form_row(form.officePhone) }}
    {{ form_row(form.cellularPhone) }}
    {{ form_row(form.faxNumber) }}
    {{ form_row(form.primaryMail) }}
    {{ form_row(form.secondaryMail) }}
</fieldset>

<fieldset>
    <legend>Tipologia contatto</legend>

    {{ form_row(form.isCustomer) }}
    {{ form_row(form.isInstaller) }}
    {{ form_row(form.isAdministrator) }}
</fieldset>
