<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Data</th>
      <th>Ora</th>
      <th>Cliente</th>
      <th>Tecnico</th>
    </tr>
  </thead>
  <tbody>
    {% for entry in result.associations %}
      <tr>
        <td>{{ entry.intervention.scheduledDate|localizeddate("medium", "none") }}</td>
        <td>{{ entry.intervention.scheduledDate|localizeddate("none", "short") }}</td>
        <td>{{ entry.intervention.customer.fullName }}</td>
        <td>{{ entry.installer.fullName }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>