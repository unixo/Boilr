<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Data/Ora</th>
      <th>Cliente</th>
      <th>Nuova data</th>
    </tr>
  </thead>
  <tbody>
    {% for entry in result.associations %}
      <tr>
        <td>
          {{ entry.intervention.scheduledDate|localizeddate("medium", "none") }}
          {{ entry.intervention.scheduledDate|localizeddate("none", "short") }}
        </td>
        <td>{{ entry.intervention.customer.fullName }}</td>
        <td>{{ entry.newScheduledDate|localizeddate("none", "short") }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>