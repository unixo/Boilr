{% extends 'BoilrBundle:Default:template5-base.html.twig' %}

{% block inner_block %}
<div class="span9">
  <div class="page-header">
    <h1>Assegnazione assistita intervanti <small>Selezione politica</small></h1>
  </div>

  <h3>Risultato associazione</h3>
  <br>
  <table class="table table-striped table-condensed">
    <thead>
      <tr>
        <th>Data</th>
        <th>Cliente</th>
        <th>Tecnico</th>
        <th># prima</th>
        <th># dopo</th>
      </tr>
    </thead>
    <tbody>
      {% for entry in results %}
        <tr>
          <td>{{ entry['intervention'].scheduledDate|localizeddate("short", "none") }}</td>
          <td>{{ entry['intervention'].customer.fullName }}</td>
          <td>{{ entry['installer']['obj'].fullName }}</td>
          <td>{{ entry['installer']['prevLoad'] }}</td>
          <td>{{ entry['installer']['load'] }}</td>
        </tr>
      {% endfor %}
    </tbody>
  </table>

  <a class="btn btn-primary" href="{{ url('intervention_equalpolicy', {'doit': true}) }}">Assegna</a>

</div>
{% endblock inner_block %}